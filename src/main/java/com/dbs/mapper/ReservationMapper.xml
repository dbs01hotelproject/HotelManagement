<mapper namespace="com.dbs.mapper.ReservationMappper">
	<!-- 根据房间名去查网络信息 -->
	<select id="queryNetworkbyroomnumber" parameterType="int"  resultType="Network">
		
	</select>
	
	
	
	<!-- 以房间号码查询房间信息 -->
	<select id="queryRoombynumber" parameterType="int" resultType="room">
		select * from t_roominformation where r_number=#{number};
	</select>
